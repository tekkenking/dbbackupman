{
    "name": "tekkenking/dbbackupman",
    "description": "Cross-DB backups (PostgreSQL/MySQL/MariaDB) with uploads, retention, and incremental options.",
    "type": "package",
    "require": {
        "laravel/framework": "12.x-dev",
        "php": "^8.1|^8.2|^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/filesystem": "^10.0|^11.0|^12.0",
        "symfony/process": "^6.4|^7.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^10.5|^11.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Tekkenking\\Dbbackupman\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tekkenking\\Dbbackupman\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "tekkenking",
            "email": "mytansill@yahoo.com"
        }
    ],
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "Tekkenking\\Dbbackupman\\DbBackupServiceProvider"
            ]
        }
    }
}
